<?xml version="1.0" encoding="utf-8"?>
<openerp>
   <data>
    
        <!-- Sequences for work order -->
       <record id="seq_work_order" model="ir.sequence.type">
           <field name="name">Work Order</field>
           <field name="code">work.order.sequence</field>
       </record>

       <record id="seq_seq_work_order" model="ir.sequence">
           <field name="name">Work Order Sequence</field>
           <field name="code">work.order.sequence</field>
           <field name="prefix">WO-</field>
           <field name="padding">5</field>
           <field name="implementation">no_gap</field>
       </record>
       
       <!-- Sequences for Job Card  -->
       <record id="seq_job_card" model="ir.sequence.type">
           <field name="name">job card</field>
           <field name="code">job.card.sequence</field>
       </record>

       <record id="seq_seq_job_card" model="ir.sequence">
           <field name="name">Job Card Sequence</field>
           <field name="code">job.card.sequence</field>
           <field name="prefix">JBC</field>
           <field name="padding">6</field>
       </record>
       
       <!-- Sequences for Contract -->
       <record id="seq_contract_card" model="ir.sequence.type">
           <field name="name">Contract Sequence</field>
           <field name="code">vehicle.contract.sequnce</field>
       </record>

       <record id="seq_seq_contract_card" model="ir.sequence">
           <field name="name">Contract Sequence</field>
           <field name="code">vehicle.contract.sequnce</field>
           <field name="prefix">CON</field>
           <field name="padding">6</field>
       </record>
       
   </data>
</openerp>